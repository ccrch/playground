  baseClass: 'brush-stroke-test',
  navDescription: '',
  navTitle: 'Brush stroke test',
}

import Layout from '../layouts/playground-layout.astro'
---

<script>
  import gsap from 'gsap'
  import DrawSVGPlugin from 'gsap/DrawSVGPlugin'

  gsap.registerPlugin(DrawSVGPlugin)

  const animation = gsap.timeline({ paused: true })

  const createAnimation = () => {
    const paths = document.querySelectorAll('.test-3 .reveal-strokes path')
    const pathsArray = gsap.utils.toArray<SVGPathElement>(paths)
    const randomizedPaths = gsap.utils.shuffle(pathsArray)

    gsap.set(paths, { drawSVG: '0% 0%' })

    randomizedPaths.forEach((path: SVGPathElement, index: number) => {
      const pathLength = DrawSVGPlugin.getLength(path)
      const duration = pathLength > 500 ? pathLength / 1330 : pathLength / 330

      animation.add(
        gsap.to(path, {
          drawSVG: gsap.utils.random(0, 1, 1) === 1 ? '-100% 0%' : '0% 100%',
          duration,
          ease: 'power1.inOut',
        }),
        index * 0.04
      )
    })
  }

  createAnimation()

  document.querySelector('.test-3')?.addEventListener('click', () => {
    animation.seek(0)
    animation.clear()
    createAnimation()
  })

  document.querySelector('.test-3')?.addEventListener('mouseenter', () => {
    animation.timeScale(1).play()
  })

  document.querySelector('.test-3')?.addEventListener('mouseleave', () => {
    animation.timeScale(3).reverse()
  })
</script>

<style lang="scss">
  .brush-stroke-test {
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
    gap: 5vw;
    padding: 12vw;

    .test-1 {
      .reveal-stroke {
        stroke-dasharray: 1;
        stroke-dashoffset: 1; /* fully visible (white = mask on) */
        transition: stroke-dashoffset 1.5s ease-in-out;
      }

      /* On hover: hide it by sliding the dash off the path */
      &:hover .reveal-stroke {
        stroke-dashoffset: 0; /* 1 = full length, so the white stroke moves off */
      }
    }

    .test-2 {
      width: 666px;
      aspect-ratio: 1;
      background: #e6f3ea;

      picture,
      img {
        width: 100%;
        height: 100%;
        object-fit: contain;
      }

      .strokes path {
        stroke-dasharray: 1;
        stroke-dashoffset: 1; /* fully visible (white = mask on) */
        transition: stroke-dashoffset 0.5s ease-out;
      }

      &:hover .strokes path {
        stroke-dashoffset: 0; /* 1 = full length, so the white stroke moves off */
      }

      &:hover image[opacity] {
        opacity: 0;
      }
    }

    .test-3 {
